    "ts-loader": "^6.2.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.2",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
